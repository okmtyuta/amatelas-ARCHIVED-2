{
  "name": "@okmtyuta/amatelas-react",
  "version": "0.0.3",
  "type": "module",
  "main": "./dist/server.js",
  "files": [
    "dist"
  ],
  "exports": {
    "./server": {
      "import": "./dist/server.js",
      "types": "./dist/types/src/components/server/index.d.ts"
    },
    "./reactive": {
      "import": "./dist/reactive.js",
      "types": "./dist/types/src/components/reactive/index.d.ts"
    }
  },
  "scripts": {
    "dev": "vite",
    "prebuild": "pnpm --filter @okmtyuta/amatelas-theme build",
